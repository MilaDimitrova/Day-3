#include <stdio.h>
#include <stdlib.h>

void primeNum(int n){
    int num[n];
    num[0] = 1;
    num[1] = 1;

    int start = 2;
    while(start <= 7){
        if(num[start] == 1){
            start++;
            continue;
        }
        num[start] = 0;

        for(int i = start * 2; i <= n; i += start){
            num[i] = 1;
        }

        start++;
    }

    for(int i = 0; i < n; i++){
        if(num[i] == 0){
            printf("%d\n", i);
        }
    }
}

int main(){
    primeNum(50);
}
