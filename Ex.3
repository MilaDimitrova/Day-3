#include <stdio.h>
#include <stdlib.h>


void swap(int *x, int *y)
{
    int temp = *x;
    *x = *y;
    *y = temp;
}

void sort(int arr[], int n){
   int i, j;
   for (i = 0; i < n-1; i++){
       for (j = 0; j < n-i-1; j++){
           if (arr[j] > arr[j+1])
              swap(&arr[j], &arr[j+1]);
       }
   }
}


void print(int arr[], int size){
    int i;
    for (i=0; i < size; i++){
        printf("%d ", arr[i]);
    }
    printf("\n");
}


int main(){
    int arr[] = {10,9,8,7,6,5};
    int num = sizeof(arr)/sizeof(arr[0]);
    sort(arr, num);
    print(arr, num);
    return 0;
}
