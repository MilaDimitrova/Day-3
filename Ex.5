#include <stdio.h>
#include <stdlib.h>

int num(int* arr, int n, int start){
    arr[0] = 0;
    arr[1] = 1;

    arr[start] = arr[start - 1] + arr[start - 2];

    if(start <= n){
        num(arr, n, start + 1);
    }

    return arr[n - 1];
}

int main(){
    int n = 11;
    int arr[n];
    printf("%d\n", num(arr, n, 2));
}

